	file = {ACM Full Text PDF:/Users/mnie/Library/Application Support/Zotero/Profiles/pduyuh73.default/zotero/storage/KUEEKZDS/Näsänen et al. - 2009 - Mobile Media in the Social Fabric of a Kindergarte.pdf:application/pdf}
