	pages = {17--23},
	file = {IEEE Xplore Abstract Record:/Users/mnie/Library/Application Support/Zotero/Profiles/pduyuh73.default/zotero/storage/WVFRSB9M/abs_all.html:text/html;IEEE Xplore Full Text PDF:/Users/mnie/Library/Application Support/Zotero/Profiles/pduyuh73.default/zotero/storage/7KZ5Z5KW/Taivalsaari et al. - 2011 - The Death of Binary Software End User Software Mo.pdf:application/pdf}
